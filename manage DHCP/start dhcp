'/home/debi/Desktop/opendhcp/opendhcpd' -v -i '/home/debi/Desktop/opendhcp/opendhcp.ini' -s /opt/opendhcp/statefile -l /opt/opendhcp/open%Y%m%d.log

сборка = g++ dhcpd.cpp -odhcpd -lpthread -lgcc_s

sudo apt install nmap

сканируем сеть:
 nmap -sP 192.168.1.* | grep -v '^Nmap scan report for 1\|^Host'

